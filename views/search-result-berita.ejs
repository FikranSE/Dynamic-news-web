<nav>
  <a href="/" class="brand">NEWS <span>magazine</span> </a>
  <a href="/">Home</a>
  <a href="/categories">Categories</a>
  <a href="/about-us">About Us</a>
  <form class="form-search-berita" action="/search-berita" method="get">
    <div class="search">
      <input name="query" type="text" class="search__input" placeholder="What news today?">
      <button type="submit" class="search__button">
          <svg class="search__icon" aria-hidden="true" viewBox="0 0 24 24">
              <g>
                  <path d="M21.53 20.47l-3.66-3.66C19.195 15.24 20 13.214 20 11c0-4.97-4.03-9-9-9s-9 4.03-9 9 4.03 9 9 9c2.215 0 4.24-.804 5.808-2.13l3.66 3.66c.147.146.34.22.53.22s.385-.073.53-.22c.295-.293.295-.767.002-1.06zM3.5 11c0-4.135 3.365-7.5 7.5-7.5s7.5 3.365 7.5 7.5-3.365 7.5-7.5 7.5-7.5-3.365-7.5-7.5z"></path>
              </g>
          </svg>
      </button>
  </div>
  </form>
</nav> 
<div class="wrapper-categories">
  <div class="mid">
    <% if (contents.length === 0) { %>
      <p class="text-center text-secondary">No results found.</p>
    <% } else { %>
  <div class="container-news bottom">
    <% contents.forEach((content)=> { %>
      <div class="news">
        <div class="pic">
          <img src="/img/<%= content.gambar %>">
        </div>
        <a  href="/detail-news/<%= content.slug %>"><%= content.title %></a>
        <p class="excerpt"><%= content.excerpt %></p>
        <div class="publisher">
          <div class="left">
            <img src="/img/man.jpg">
            <span><%= content.username %></span>
          </div>
          <div class="right">
            <span><%= moment(content.created_at).format('LL') %></span>
            <a href="" class="text-decoration-none"><%= content.kategori %></a>
          </div>
        </div>
      </div>
      <% }); %>
      <% } %>
  </div>
</div>