<div class="top w-100 mt-5 ps-2 mx-3 d-flex align-items-center justify-content-start">
  <form action="/search-admin">
    <div class="group">
      <button type="submit" class="btn-search-admin">
        <svg class="icon" aria-hidden="true" viewBox="0 0 24 24"><g><path d="M21.53 20.47l-3.66-3.66C19.195 15.24 20 13.214 20 11c0-4.97-4.03-9-9-9s-9 4.03-9 9 4.03 9 9 9c2.215 0 4.24-.804 5.808-2.13l3.66 3.66c.147.146.34.22.53.22s.385-.073.53-.22c.295-.293.295-.767.002-1.06zM3.5 11c0-4.135 3.365-7.5 7.5-7.5s7.5 3.365 7.5 7.5-3.365 7.5-7.5 7.5-7.5-3.365-7.5-7.5z"></path></g></svg>
      </button>
      <input placeholder="Search" type="search" class="input">
    </div>
  </form>
  <div class="add-kat-wrap">
    <form action="/add-publisher" method="POST">
      <div class="input-group">
        <input  class="rounded-end-0" type="text" name="username" placeholder="Username" id="input-field">
        <input  class="rounded-start-0 border-start" type="text" name="password" placeholder="Password" id="input-field">
        <input  class="rounded-start-0 border-start" type="text" name="confirm_password" placeholder="Confirm Password" id="input-field">
        <button type="submit" class="submit-button"><span>ADD</span></button>
      </div>
    </form>
   </div>
  
</div>
<div class="wrapper-content-admin p-4">
    <div class="container-table-admin ">
      <ul class="list-scroll">
        <% users.forEach((user)=> { %>
        <li>
          <div class="col col-1" >
            <% if (user.avatar) { %>
              <img src="/img/<%= user.avatar %>" id="avatar-preview" >
            <% } else { %>
              <img src="/img/admin.jpeg" id="avatar-preview" >
            <% } %>
          </div>
          <div class="col col-2 excerpt" ><%= user.username %></div>
          <div class="col col-3" ><%= user.password %></div>
          <div class="col col-4 " ><%= moment(user.created_at).format('LL LT') %></div>
          <div class="col col-5 " >
            <a href="" class="btn-view">Edit</a>
            <a href="/delete-user/<%= user.admin_id %>" class="btn-view bg-danger ms-2" onclick=" return confirmDelete()">Delete</a>
          </div>
        </li>
        <% }); %>
      </ul>
    </div>
</div>
</div>


<script>
  function confirmDelete() {
    return confirm("Yakin menghapus?");
  }

</script>